{
	"name": "run-club-tracker-frontend",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" -- && cpx netlify.toml dist/",
		"preview": "vite preview",
		"build-only": "vite build",
		"type-check": "vue-tsc --build",
		"lint": "eslint . --fix",
		"format": "prettier --write src/",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{js,ts,vue}": [
			"eslint --fix",
			"prettier --write"
		]
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.11",
		"echarts": "^6.0.0",
		"oh-vue-icons": "^1.0.0-rc3",
		"tailwindcss": "^4.1.11",
		"vue": "^3.5.17",
		"vue-router": "^4.5.1"
	},
	"devDependencies": {
		"@tsconfig/node22": "^22.0.2",
		"@types/echarts": "^5.0.0",
		"@types/node": "^22.15.32",
		"@vitejs/plugin-vue": "^6.0.0",
		"@vue/eslint-config-prettier": "^10.2.0",
		"@vue/eslint-config-typescript": "^14.5.1",
		"@vue/tsconfig": "^0.7.0",
		"cpx": "^1.5.0",
		"eslint": "^9.29.0",
		"eslint-plugin-vue": "~10.2.0",
		"husky": "^9.1.7",
		"jiti": "^2.4.2",
		"lint-staged": "^16.1.2",
		"npm-run-all": "^8.0.4",
		"prettier": "3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"typescript": "~5.8.0",
		"vite": "^7.0.0",
		"vite-plugin-vue-devtools": "^7.7.7",
		"vue-tsc": "^2.2.10"
	}
}
